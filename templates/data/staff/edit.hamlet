
<header>
  <md-icon-button href=@{DataR $ EmployeeR sid} aria-label=_{MsgBack}>
    <md-icon>arrow_back

  <span.md-typescale-title-large>
    _{MsgStaff}

<main>

  ^{widgetBanner msgs}

  <form method=post action=@{DataR $ EmployeeR sid} enctype=#{et} novalidate ##{idFormStaff}>
    ^{fw}
    <div.form-actions>
      <md-text-button type=button href=@{DataR $ EmployeeR sid}>
        _{MsgCancel}
      <md-filled-button type=submit>
        _{MsgSave}

^{widgetSnackbar msgs}
