
<header.primary>
  ^{widgetMenu}

  <h1.title-large>
    _{MsgDocumentation}

  ^{widgetAccount}

<main>

  ^{widgetBanner msgs}

  <section>
    <h2.headline-medium>_{MsgAppName}
    <h3.body-large>
      _{MsgAppDescription}

    <h2.headline-small>
      _{MsgOverview}
    <p.body-medium>
      #{t $ MsgDoc001 (m MsgAppName) (r HomeR)}

  <section>
